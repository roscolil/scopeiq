# Environment Variables Template
# Copy this file to .env and fill in your actual values

# AWS Configuration
VITE_AWS_REGION=ap-southeast-2
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
VITE_S3_BUCKET_NAME=your_s3_bucket_name_here

# API Keys
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# App Configuration
VITE_NODE_ENV=development
VITE_APP_URL=http://localhost:3000

# SES Integration (added after deployment)
# Get this URL from amplify_outputs.json custom.sendContactEmailUrl after deploying
# VITE_SEND_CONTACT_EMAIL_URL=https://your-function-url.lambda-url.ap-southeast-2.on.aws/
